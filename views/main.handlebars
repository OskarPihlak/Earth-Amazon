                    <div class="chart">
                        <form method="post">
                            <div class="quantity">
                                <p class="quantity-cartridge-info">{{this.cartridge.yellow.name}} Storage: </p>
                                <input class="cartridge-storage" name="inc_storage_count" type="number" min="1" step="1" value="inc_supply.pr_tln_12k_HP_MFP252n_Yellow_supplies.supply">
                                <div class="quantity-nav">
                                    <div class="quantity-button quantity-up">+</div>
                                    <div class="quantity-button quantity-down">-</div>
                                </div>
                            </div>
                            <input type="hidden" class="printer-title" name="inc_storage_title" value="{{this.name}}">
