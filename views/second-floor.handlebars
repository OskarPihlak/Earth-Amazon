<style>
    body {
        background-color: #0e0a0f;
    }
</style>

<div class="container" id="container" style="background-color: #0A0D13">
    <div class="printer">
        <div class="title"><a href="//{{this.ip}}/" target="_blank">{{this.name}}<br>{{this.ip}}</a></div>
        <section class="black-precentage-section printer-section">
            <article class="printer-map">
                <div>

                    <div class="led-box led-positioning" style="top: 450px;left: 1260px;">
                        {{#ifEquals printer_name 'pr_tln_2k_HPPro8610'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>


                    <div class="led-box led-positioning" style="top: 309px;left: 1211px;">
                        {{#ifEquals printer_name 'pr_tln_2k_HP476dn'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>



                    <div class="led-box led-positioning" style="top: 571px;left: 542px;">
                        {{#ifEquals printer_name 'pr_tln_2k_HP1320n'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>
                    <img src="/images/2nd-floor.png" width="1640" height="1000" alt="10-floor">

                </div>
            </article>
        </section>
        <form action="/">
            <button>BACK</button>
        </form>
    </div>
</div>
