<style>
    body {
        background-color: #0e0a0f;
    }
</style>

<div class="container" id="container" style="background-color: #0A0D13">
    <div class="printer">
        <div class="title"><a href="//{{this.ip}}/" target="_blank">{{this.name}}<br>{{this.ip}}</a></div>
        <section class="black-precentage-section printer-section">
            <article class="printer-map">
                <div>

                    <div class="led-box led-positioning" style="    top: 250px;
    left: 1070px;">
                        {{#ifEquals printer_name 'pr_tln_10k_HP2420n'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <div class="led-box led-positioning" style="top: 260px;
    left: 1100px;">
                        {{#ifEquals printer_name 'pr_tln_10k_HPM476dn'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>
                    <img src="/images/10th-floor.png" width="1640" height="1000" alt="10-floor">

                </div>
            </article>
        </section>
        <form action="/">
            <button>BACK</button>
        </form>
    </div>
    </div>
</div>