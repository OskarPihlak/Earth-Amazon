<style>
    body {
        background-color: #0e0a0f;
    }
</style>

<div class="container" id="container" style="background-color: #0A0D13">
    <div class="printer">
        <div class="title"><a href="//{{this.ip}}/" target="_blank">{{this.name}}<br>{{this.ip}}</a></div>
        <section class="black-precentage-section printer-section">
            <article class="printer-map">
                <div>

                    <div class="led-box led-positioning" style="top: 364px;left: 1309px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HP5225dn'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>


                    <div class="led-box led-positioning" style="top: 364px;left: 1283px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HP400'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <div class="led-box led-positioning" style="top: 700px;left: 1273px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HP_P3005'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <div class="led-box led-positioning" style="top: 692px;left: 881px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HP521dn'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <div class="led-box led-positioning" style="top: 626px;left: 1214px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HP_CP5225n'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <div class="led-box led-positioning" style="top: 600px;left: 1214px;">
                        {{#ifEquals printer_name 'pr_tln_4k_HPLJ_5200'}}
                            <div class="led-green"></div>
                        {{else}}
                            <div class="led-blue"></div>
                        {{/ifEquals}}
                    </div>

                    <img src="/images/4th-floor.png" width="1640" height="1000" alt="10-floor">

                </div>
            </article>
        </section>
    </div>
</div>
<form action="/">
    <button>BACK</button>
</form>